{
    "name": "brylo-cli",
    "version": "0.1.0",
    "description": "CLI open source ultra propre",
    "main": "bin/index.js",
    "type": "module",
    "scripts": {
        "build": "tsc",
        "lint": "eslint . --ext .ts",
        "format": "prettier --write .",
        "test": "jest",
        "prepare": "husky install",
        "release": "semantic-release",
        "commit": "git-cz"
    },
    "bin": {
        "brylo": "bin/index.js"
    },
    "keywords": [
        "cli",
        "open source",
        "typescript"
    ],
    "author": "RenanYhuel",
    "license": "MIT",
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "devDependencies": {
        "@commitlint/cli": "^19.0.0",
        "@commitlint/config-conventional": "^19.0.0",
        "@eslint/js": "^9.34.0",
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/git": "^10.0.1",
        "@types/jest": "^30.0.0",
        "commitizen": "^4.3.1",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^9.0.0",
        "git-cz": "^4.9.0",
        "husky": "^9.0.0",
        "jest": "^30.1.1",
        "lint-staged": "^15.0.0",
        "prettier": "^3.0.0",
        "semantic-release": "^24.2.7",
        "ts-jest": "^29.4.1",
        "typescript": "^5.0.0",
        "typescript-eslint": "^8.41.0"
    }
}
